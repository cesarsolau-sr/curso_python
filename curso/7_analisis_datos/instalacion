# Prueba funcional de Num
# Py
import numpy as np
arr = np.array([1, 2, 3, 4, 5])
print(arr.mean())
#3.0

# Prueba funcional de Pandas
import pandas as pd
df = pd.DataFrame({'A': [1, 2, 3], 'B': [4, 5, 6]})
print(df.describe())
